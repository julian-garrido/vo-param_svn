    <jxb:bindings node="*">
        <jxb:globalBindings>
       <xjc:javaType name="org.joda.time.DateTime" xmlType="xsd:dateTime"
        adapter="net.ivoa.DateTimeXmlAdapter" />
       </jxb:globalBindings>
     </jxb:bindings>   
 -->
     <jxb:bindings schemaLocation="../schema/UWS2-V1.1.xsd"
      node="/xs:schema"
   >
